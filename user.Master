            #modalOverlay {
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.7);
            z-index: 9999;
            padding-top:200px;
        }

        /* Modal Box */
        #customModal {
            position: fixed;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            width: 50%;
            background: #fff;
            padding: 20px;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.2);
            border-radius: 8px;
            z-index: 10000;
        }

        /* Close Button */
        .closeButton {
            position: absolute;
            top: 10px;
            right: 10px;
            background: red;
            color: white;
            border: none;
            padding: 5px 10px;
            border-radius: 50%;
            cursor: pointer;
        }

        /* Modal Content */
        .modal-body {
            margin-top: 20px;
        }
        .responsive-image {
    max-width: 100%; /* Ensures image doesn't exceed modal width */
    height: auto;    /* Maintains image's aspect ratio */
    object-fit: contain; /* Ensures the image fits properly without cropping */
}
