        nav {
            position: absolute;
            top: 0;
            right: 0;
            padding: 5px 16px;
            display: flex;
            justify-content: space-between;
            align-items: start;
        }

        /* profile menu */

        .profile {
            position: relative;
            display: flex;
            justify-content: flex-end;
            align-items: center;
            gap: 12px;
            cursor: pointer;
            text-align: end;
        }

            .profile h3 {
                text-align: end;
                line-height: 1;
                margin-bottom: 4px;
                font-weight: 600;
            }

            .profile p {
                line-height: 1;
                font-size: 14px;
                margin: 0;
                color: #ffffff
            }

            .profile .img-box {
                position: relative;
                width: 48px;
                height: 48px;
                border-radius: 50%;
                /*overflow: hidden;*/
            }

                .profile .img-box img {
                    position: absolute;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                    object-fit: cover;
                }

        /* menu (the right one) */

        .profile-menu {
            position: absolute;
            top: calc(100% + 8px);
            right: 70px;
            width: 200px;
            min-height: 100px;
            background: #fff;
            box-shadow: 0 10px 20px rgba(0, 0, 0, .2);
            opacity: 0;
            transform: translateY(-10px);
            visibility: hidden;
            transition: 300ms;
        }

            .profile-menu::before {
                content: '';
                position: absolute;
                top: -10px;
                right: 14px;
                width: 20px;
                height: 20px;
                background: #fff;
                transform: rotate(45deg);
                z-index: -1;
            }

            .profile-menu.active {
                opacity: 1;
                transform: translateY(0);
                visibility: visible;
                z-index: 999;
            }

            /* menu links */

            .profile-menu ul {
                position: relative;
                right: 0px;
                display: flex;
                flex-direction: column;
                z-index: 1000;
                background: #fff;
                justify-content: center;
                padding: 0;
                margin: 0;
                width: fit-content;
            }

                .profile-menu ul li {
                    list-style: none;
                }

                    .profile-menu ul li:hover {
                        background: #eee;
                    }

                    .profile-menu ul li a {
                        text-decoration: none;
                        color: #000;
                        display: flex;
                        align-items: center;
                        padding: 15px 20px;
                        gap: 6px;
                        font-size: 1em;
                        border-bottom: 1px solid #ccc;
                    }

                        .profile-menu ul li a i {
                            font-size: 1.2em;
                        }

        .members-image {
            border-radius: 50%;
        }

        .manage-members-div {
            max-width: 90% !important;
            height: 100% !important;
            margin: 0;
        }

        @media (max-width: 912px) {
            .profile-menu ul {
                right: 0px !important;
            }
        }

        .profile .user span {
            font-family: 'Red Hat Display', Sans-serif !important;
        }

        .profile-menu ul li .gray-color {
            color: #808080 !important;
        }

        .goog-te-combo {
            padding: 10px 20px !important;
        }

        #google_translate_element {
            margin-right: 20px;
        }

            .goog-logo-link, #google_translate_element span {
                display: none !important;
            }

        .goog-te-gadget {
            color: transparent !important;
        }

        .common-heading img {
            width: 128px !important;
